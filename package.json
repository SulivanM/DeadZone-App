{
  "name": "tlsdzapp",
  "version": "0.0.1",
  "description": "Simple, customizable client with built in flash player to play The Last Stand: Dead Zone Revive.",
  "main": "main.js",
  "repository": "https://github.com/SulivanM/DeadZone-App",
  "author": "TLSDZApp",
  "contributors": [
    {"name": "SulivanM", "url": "https://github.com/SulivanM"}
  ],
  "directories": {
    "flashver": "flashver"
  },
  "keywords": [
    "tlsdz",
    "tlsdz app",
    "thelaststanddeadzone reborn",
    "thelaststanddeadzone revive",
    "thelaststanddeadzone back"
  ],
  "funding": {
    "type": "Buy Me a Coffee",
    "url": "https://buymeacoffee.com/sulivanm"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . \"Dead Zone Revive\" --out=build --platform=all --icon=./resources/logo.ico --extraResource=resources/logo.png",
    "buildx32": "electron-packager . \"Dead Zone Revive\" --out=build --platform=all --arch=ia32 --icon=./resources/logo.ico --extraResource=resources/logo.png",
    "pack": "build --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^11.4.9",
    "electron-builder": "^22.11.7",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^3.2.7"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1"
  }
}